<div class="container">
    <div class="row">
        <%= render 'sidebar' %>
        <div class="col-md-8">
            <div class="card">
                <div class="card-header">
                    Upload your Images
                </div>
                <div class="card-body">
                    <%= form_with(model: @property, local: true) do |f| %>
                        <%= button_to 'ファイル選択',  class: 'btn btn-info' %>

                        <div class="text-center">
                            <%= f.submit :Add_Photo, class: 'btn btn-primary' %>
                        </div>
                    <% end %>
                </div>
            </div>
        </div>
    </div>
</div>